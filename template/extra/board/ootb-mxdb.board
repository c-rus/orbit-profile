; ----- 7-Segment Displays -----------------------------------------------------
; NOTE: ON with low logic level, OFF with high logic level.
;              A
;          ========
;     F  //      //  B
;       //  G   //
;       ========
;  E  //      //  C
;    //      //
;    ========    * DP
;       D
; ------------------------------------------------------------------------------
; PIN_H10 = __              ; U8A  Seven Segment Digit U8[0] 3.3-V
; PIN_F8  = __              ; U8B  Seven Segment Digit U8[1] 3.3-V
; PIN_E8  = __              ; U8C  Seven Segment Digit U8[2] 3.3-V
; PIN_J12 = __              ; U8D  Seven Segment Digit U8[3] 3.3-V
; PIN_H9  = __              ; U8E  Seven Segment Digit U8[4] 3.3-V
; PIN_J10 = __              ; U8F  Seven Segment Digit U8[5] 3.3-V
; PIN_L11 = __              ; U8G  Seven Segment Digit U8[6] 3.3-V
; PIN_E9  = __              ; U8DP Seven Segment Digit U8[7], DP 3.3-V

; PIN_D9  = __              ; U7A  Seven Segment Digit U7[0] 3.3-V
; PIN_C9  = __              ; U7B  Seven Segment Digit U7[1] 3.3-V
; PIN_G12 = __              ; U7C  Seven Segment Digit U7[2] 3.3-V
; PIN_L10 = __              ; U7D  Seven Segment Digit U7[3] 3.3-V
; PIN_K10 = __              ; U7E  Seven Segment Digit U7[4] 3.3-V
; PIN_J9  = __              ; U7F  Seven Segment Digit U7[5] 3.3-V
; PIN_E6  = __              ; U7G  Seven Segment Digit U7[6] 3.3-V
; PIN_E12 = __              ; U7DP Seven Segment Digit U7[7], DP 3.3-V